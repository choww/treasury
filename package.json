{
  "name": "treasury",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "transpile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run transpile",
    "start": "node ./dist/bin/www",
    "dev": "nodemon ./src/bin/www"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "12"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv-safe": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "googleapis": "^39.2.0",
    "mongoose": "^5.9.16",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.3",
    "rimraf": "^3.0.2"
  }
}
