{
  "name": "treasury",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "transpile": "babel ./src --out-dir dist",
    "build": "npm run clean && npm run transpile",
    "clean": "rimraf dist",
    "start": "node ./dist/app",
    "dev": "npm run build && nodemon ./dist/app"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "12"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv-safe": "^8.2.0",
    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.27.0",
    "mongoose": "^5.9.16",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.20",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2"
  }
}
